{
    "name": "@smooai/logger",
    "version": "0.0.0",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist/**"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/SmooAI/logger.git"
    },
    "scripts": {
        "build": "pnpm createEntryPoints && pnpm dlx cross-env tsup && cp src/decycle.cjs dist/",
        "createEntryPoints": "npx vite-node ../utils/src/scripts/createEntryPoints.ts -i \"src/**/*.ts\"",
        "lint": "eslint src/",
        "lint:fix": "pnpm lint -- --fix",
        "test": "vitest run --passWithNoTests",
        "typecheck": "tsc --noEmit --skipLibCheck",
        "watch": "tsup --watch",
        "check-all": "pnpm run typecheck && pnpm run lint && pnpm run test && pnpm run build"
    },
    "dependencies": {
        "@aws-sdk/client-sqs": "^3.504.0",
        "@opentelemetry/api": "^1.8.0",
        "browser-dtector": "^4.1.0",
        "cross-fetch": "^4.0.0",
        "dayjs": "^1.11.13",
        "esm-utils": "^4.2.1",
        "json-stable-stringify": "^1.2.1",
        "lodash": "^4.17.21",
        "serialize-error": "^11.0.3",
        "source-map-support": "^0.5.21",
        "uuid": "^9.0.0"
    },
    "devDependencies": {
        "@smooai/config-eslint": "workspace:*",
        "@smooai/config-typescript": "workspace:*",
        "@smooai/config-typescript-extended": "workspace:*",
        "@types/aws-lambda": "^8.10.119",
        "@types/json-stable-stringify": "^1.0.36",
        "@types/lodash": "^4.14.195",
        "@types/node": "^18.11.9",
        "@types/source-map-support": "^0.5.10",
        "@types/uuid": "^9.0.2",
        "chalk": "^5.3.0",
        "tsup": "^8.4.0",
        "vite": "^5.4.14",
        "vite-tsconfig-paths": "^5.1.4",
        "vitest": "^3.0.7"
    },
    "engines": {
        "node": ">=20.0.0"
    },
    "publishConfig": {
        "access": "public"
    }
}
